{
  "redtail": {
    "api_base": {
      "protocol": "http",
      "hostname": "localhost",
      "port": "3000",
      "pathname": ""
    },
    "paths": {
      "config": "/config",
      "neighborhoods": "/neighborhoods",
      "rodents": "/rodents",
      "establishments": "/establishments",
      "agencies": "/agencies",
      "sources": "/sources",
      "statuses": "/statuses",
      "geocodes": "/geocodes"
    },
    "map": {
      "tile_url": "https://api.mapbox.com/styles/v1/mapbox/streets-v10/tiles/256/{z}/{x}/{y}?access_token={accessToken}",
      "options": {
        "accessToken": "pk.eyJ1IjoiY3RoaWVyZXIiLCJhIjoiY2l3NGdoaTZkMDBrbzJ0cGd2eGU2aGs5MiJ9.aHADhfD8LYo7TvpGbWfirA"
      },
      "initial_view": [39.299236, -76.609383],
      "initial_zoom": 13
    }
  },
  "geocoder": {
    "remote": {
      "url": "https://geocoding.geo.census.gov/geocoder/locations/address",
      "query": {
        "benchmark": "Public_AR_Current"
      }
    },
    "params": {
      "street": "street",
      "city": "city",
      "zip": "zip",
      "state": "state"
    }
  },
  "logger": {
    "name": "redtail",
    "level": "info",
    "src": false
  },
  "models": {
    "db": {
      "database": "redtail",
      "username": "redtail",
      "password": "",
      "options": {
        "dialect": "mysql",
        "host": "localhost",
        "port": 3306
      }
    }
  },
  "neighborhoods": {
    "populate": {
      "remote": {
        "method": "GET",
        "url": "https://data.baltimorecity.gov/resource/h3fx-54q3.json",
        "query": {
          "$select": "label,shape_area"
        }
      },
      "mapping": {
        "label": {
          "column": "name"
        },
        "shape_area": {
          "column": "area"
        }
      }
    }
  },
  "establishments":{
    "populate": {
      "remote": {
        "method": "GET",
        "url": "https://data.baltimorecity.gov/resource/abuv-d2r2.json",
        "query": {
          "$select": "name,zipcode,neighborhood,location_1_location,location_1_city,location_1_state",
          "$limit": 5000
        }
      },
      "mapping": {
        "name": {
          "column": "name"
        },
        "zipcode": {
          "column": "zip"
        },
        "neighborhood": {
          "ref": "belongsTo",
          "model": "Neighborhood",
          "column": "name",
          "create": false
        },
        "location_1_location": {
          "column": "street"
        },
        "location_1_city": {
          "column": "city"
        },
        "location_1_state": {
          "column": "state"
        }
      }
    }
  },
  "rodents": {
    "populate": {
      "remote": {
        "method": "GET",
        "url": "https://data.baltimorecity.gov/resource/q7s2-a6pd.json",
        "query": {
          "$select": "agency,neighborhood,address,zip,methodreceived,status,outcome,createddate,updateddate",
          "$where": "codedescription='HCD-Rodents' and lower(outcome) not like '%no active rodent sign%' and lower(outcome) not like '%condition does not exist%' and lower(outcome) not like '%no entry%' and lower(outcome) not like '%not a violation%' and lower(outcome) not like '%invalid address%' and lower(outcome) not like '%no cause for action%'",
          "$limit": 5000
        }
      },
      "mapping": {
        "agency": {
          "ref": "belongsTo",
          "model": "Agency",
          "column": "name",
          "create": true
        },
        "neighborhood":{
          "ref": "belongsTo",
          "model": "Neighborhood",
          "column": "name",
          "create": false
        },
        "address": {
          "column": "street"
        },
        "city": {
          "column": "city",
          "default": "Baltimore"
        },
        "state": {
          "column": "state",
          "default": "MD"
        },
        "zip": {
          "column": "zip"
        },
        "methodreceived": {
          "ref": "belongsTo",
          "model": "Source",
          "column": "title",
          "create": true
        },
        "status": {
          "ref": "belongsTo",
          "model": "Status",
          "column": "title",
          "create": true
        },
        "outcome": {
          "column": "notes"
        },
        "createddate": {
          "column": "created_at"
        },
        "updateddate": {
          "column": "updated_at"
        }
      }
    }
  }
}
