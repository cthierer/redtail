{
  "name": "redtail",
  "version": "1.0.0",
  "description": "",
  "main": "app/index.js",
  "scripts": {
    "prepublish": "run-p build",
    "prebuild": "run-p test",
    "build": "run-p build:*",
    "prebuild:client": "run-p clean:client",
    "build:client": "BABEL_ENV=client run-p build:client:*",
    "build:client:styles": "node-sass --output dist/styles site/styles",
    "build:client:static": "mkdir dist && cp -r site/* dist",
    "build:client:script": "webpack",
    "build:client:tags": "riot --type babel src dist/scripts/tags.js",
    "prebuild:server": "run-p clean:server",
    "build:server": "BABEL_ENV=server babel src -d app",
    "prebuild:doc": "run-p clean:doc",
    "build:doc": "esdoc -c esdoc.conf.json",
    "clean": "run-p clean:*",
    "clean:client": "rimraf dist",
    "clean:server": "rimraf app",
    "clean:doc": "rimraf doc",
    "clean:coverage": "rimraf coverage",
    "test": "run-s test:lint test:unit",
    "pretest:unit": "run-p clean:coverage",
    "test:unit": "nyc mocha --require ./test/entry --recursive ./test/unit",
    "posttest:unit": "nyc --lines 90 --statements 90 report --reporter=lcov --reporter=json-summary",
    "test:lint": "eslint --ext .js src",
    "watch": "nodemon --watch src --exec './node_modules/.bin/run-s build restart'",
    "bin:initData": "run-s bin:initTables bin:importData",
    "bin:refreshData": "run-s bin:initTables bin:populateData",
    "bin:initTables": "node app/bin/initTables",
    "bin:populateData": "node app/bin/populateData",
    "bin:importData": "node app/bin/importData sql/data.sql"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cthierer/redtail.git"
  },
  "author": "Chris Thierer <cthierer1@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cthierer/redtail/issues"
  },
  "homepage": "https://github.com/cthierer/redtail#readme",
  "private": true,
  "engines": {
    "node": "~6.9.1"
  },
  "directories": {
    "lib": "./app/"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-async-to-bluebird": "^1.1.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-es2015-riot": "^1.1.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "esdoc": "^0.4.8",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.1.2",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "nyc": "^9.0.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "string-replace-loader": "^1.0.5",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "bootstrap": "^4.0.0-alpha.5",
    "bunyan": "^1.8.5",
    "config": "^1.24.0",
    "express": "^4.14.0",
    "form-serialize": "^0.7.1",
    "jquery": "^3.1.1",
    "leaflet": "^1.0.2",
    "moment": "^2.17.0",
    "mysql": "^2.12.0",
    "numeral": "^1.5.6",
    "octicons": "^5.0.1",
    "popsicle": "^8.2.0",
    "riot": "^2.6.7",
    "sequelize": "^3.27.0",
    "tether": "^1.3.7",
    "uuid": "^3.0.0"
  }
}
